{
  "name": "rocker-code",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:shell": "ng serve shell --port 4200",
    "start:editor": "ng serve editor --port 4201",
    "start:rocky": "ng serve rocky --port 4202",
    "start:challenger": "ng serve challenger --port 4203",
    "dev": "concurrently -n shell,editor,rocky,challenger \"pnpm start:shell\" \"pnpm start:editor\" \"pnpm start:rocky\" \"pnpm start:challenger\"",
    "build": "ng build",
    "lint": "pnpm ng lint shell && pnpm ng lint editor && pnpm ng lint rocky && pnpm ng lint challenger && pnpm ng lint shared && pnpm ng lint theme",
    "format": "prettier --write .",
    "stylelint": "stylelint \"**/*.scss\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "prepare": "husky install",
    "postinstall": "playwright install chromium && node tools/patch-monaco-font.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@material/web": "^1.0.4",
    "@ngrx/signals": "^19.0.0",
    "@softarc/native-federation-node": "^2.0.10",
    "es-module-shims": "^1.5.12",
    "monaco-editor": "^0.55.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.22.5",
    "@analogjs/vitest-angular": "^1.1.1",
    "@angular-architects/native-federation": "^19.0.0",
    "@angular-devkit/architect": "^0.1902.19",
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular-eslint/builder": "^19.0.2",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular-eslint/schematics": "^19.0.2",
    "@angular-eslint/template-parser": "^19.0.2",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.0",
    "@playwright/test": "^1.50.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.5",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "ng-packagr": "^19.2.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.7",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^2.1.9"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mts,cts,html}": "eslint --fix",
    "*.scss": "stylelint --fix",
    "*.{md,json,yaml,yml,scss,css,ts,tsx,js,jsx}": "prettier --write"
  }
}
